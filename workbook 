let Selected = '{SelectedTable}';
search *
| where $table == Selected
| extend 
    SourceSystem = iif(isnull(SourceSystem), "N/A", tostring(SourceSystem)),
    Computer = iif(column_ifexists("Computer", "") == "", "N/A", tostring(Computer)),
    DeviceVendor = iif(column_ifexists("DeviceVendor", "") == "", "N/A", tostring(DeviceVendor)),
    DeviceProduct = iif(column_ifexists("DeviceProduct", "") == "", "N/A", tostring(DeviceProduct)),
    _ResourceId = iif(column_ifexists("_ResourceId", "") == "", "N/A", tostring(_ResourceId))
| summarize Events = count() by SourceSystem, Computer, DeviceVendor, DeviceProduct, _ResourceId
| order by Events desc